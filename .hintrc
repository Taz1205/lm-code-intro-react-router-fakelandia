{
  "extends": [
    "development"
  ],
  "hints": {
    "typescript-config/is-valid": "off",
    "typescript-config/consistent-casing": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "font-synthesis",
          "@layer"
        ]
      }
    ],
    "disown-opener": "off",
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 114",
    "not chrome <= 114",
    "not edge <= 114",
    "not opera <= 99",
    "not samsung <= 21"
  ]
}